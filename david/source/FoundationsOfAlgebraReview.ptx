  <subsection>
    <title>Factoring</title>
    <p>
      Factor each of the following expressions:
    </p>
    
    <exercises><exercise>
      <statement>
        <p>
          <m>18 - 3a</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>3(6-a)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>4a^2 + 6a^3</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>2a^2(2+3a)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>15b + 24c - 36d</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>3(5b+8c-12d)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>5xy - 10x^2y^3 - 15x^4y^5</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>5xy(1-2xy^2-3x^3y^4)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>10j^2 - 20j + 30</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>10(j^2-2j+3)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>3e^2 - 5e</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>e(3e-5)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>2f^3 - 10f^2 + f</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>f(2f^2-10f+1)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>14g^5 + 28g^3</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>7g^3(2g^2+4)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>2h^2i - 6h^4i^2</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>2h^2i(1-3h^2i)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>24a^2b^2c - 18abc^2 + 9a^3b^3c^3</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>3abc(8ab-9c+3a^2b^2c^2)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>a^2 + 12a + 35</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>(a+5)(a+7)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>b^2 - 11b + 30</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>(b-5)(b-6)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>y^2 + 6y - 55</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>(y+11)(y-5)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>c^2 - 3c - 40</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>(c-8)(c+5)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>d^2 + 4d - 45</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>(d+9)(d-5)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>-15e + e^2 + 36</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>(e-12)(e-3)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>2x^2 + 2x - 84</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>2(x+7)(x-6)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>x^3 + 21x^2 + 54x</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>x(x+18)(x+3)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>f^2 - 81</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>(f+9)(f-9)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>g^2 - 36</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>(g+6)(g-6)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>3h^2 - 147</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>3(h+7)(h-7)</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
        <p>
          <m>4a^2 - 324</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>4(a+9)(a-9)</m>
        </p>
      </solution>
    </exercise>
    
  </exercises></subsection>
