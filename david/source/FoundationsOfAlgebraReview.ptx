    <exercise>
      <statement>
        <p>
          <m>\frac{30m^5}{15m^6}</m>
        </p>
      </statement>
      <solution>
        <p>
          <m>\frac{2}{m}</m>
        </p>
      </solution>
    </exercise>
    
    <exercise>
      <statement>
