        <tabular>
          <row>
            <cell><m>x</m></cell>
            <cell><m>f(x)</m></cell>
          </row>
          <row bottom="minor">
            <cell />
            <cell />
          </row>
          <row>
            <cell><m>-8</m></cell>
            <cell><m>81</m></cell>
          </row>
          <row>
            <cell><m>-6</m></cell>
            <cell><m>49</m></cell>
          </row>
          <row>
            <cell><m>-4</m></cell>
